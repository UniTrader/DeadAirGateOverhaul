<add sel="//ships">
<!-- MIN -->
  <ship id="ministry_builder_xl" group="min_builder_xl">
    <category tags="[builder, mission]" faction="[ministry]" size="ship_xl"/>
    <pilot>
      <select faction="ministry" tags="traderpilot"/>
    </pilot>
    <drop ref="ship_large_civilian"/>
