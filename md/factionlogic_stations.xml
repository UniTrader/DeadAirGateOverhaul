<?xml version="1.0" encoding="utf-8"?>
<diff name="FactionLogic_Stations" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="md.xsd">
<!-- Wharfs -->
<replace sel="//cue[@name='Analyse_Stations']/actions/do_if[@value='[faction.argon, faction.antigone, faction.paranid, faction.holyorder, faction.alliance, faction.teladi, faction.ministry].indexof.{$Faction}'][2]/@value">[faction.alliance, faction.antigone, faction.buccaneers, faction.court, faction.fallensplit, faction.hatikvah, faction.ministry, faction.scaleplate].indexof.{$Faction}</replace>
<replace sel="//cue[@name='Analyse_Stations']/actions/do_elseif[@value='$Faction == faction.freesplit']">
	<do_elseif value="[faction.argon, faction.freesplit, faction.holyorder, faction.paranid, faction.teladi].indexof.{$Faction}">
      <set_value name="$DesiredWharfs" exact="2"/>
    </do_elseif>
</replace>
<!-- Shipyards -->
<replace sel="//cue[@name='Analyse_Stations']/actions/do_if[@value='[faction.argon, faction.antigone, faction.paranid, faction.holyorder, faction.alliance, faction.teladi, faction.ministry].indexof.{$Faction}'][1]/@value">[faction.alliance, faction.antigone, faction.argon, faction.buccaneers, faction.court, faction.fallensplit, faction.hatikvah, faction.holyorder, faction.ministry, faction.paranid, faction.scaleplate, faction.teladi].indexof.{$Faction}</replace>
<!-- Equipment Docks -->
<replace sel="//cue[@name='Analyse_Stations']/actions/do_if[@value='$Faction == faction.argon']/@value">[faction.argon, faction.holyorder, faction.paranid, faction.teladi].indexof.{$Faction}</replace>
<replace sel="//cue[@name='Analyse_Stations']/actions/do_elseif[@value='[faction.antigone, faction.paranid, faction.holyorder, faction.teladi, faction.ministry].indexof.{$Faction}']/@value">[faction.alliance, faction.antigone, faction.buccaneers, faction.court, faction.fallensplit, faction.hatikvah, faction.ministry, faction.scaleplate].indexof.{$Faction}</replace>
<!-- Trade Stations -->
<replace sel="//cue[@name='Analyse_Stations']/actions/do_elseif[@value='[faction.argon, faction.antigone, faction.hatikvah, faction.paranid, faction.holyorder].indexof.{$Faction}']/@value">[faction.alliance, faction.antigone, faction.argon, faction.buccaneers, faction.court, faction.fallensplit, faction.freesplit, faction.hatikvah, faction.holyorder, faction.ministry, faction.paranid, faction.scaleplate, faction.split].indexof.{$Faction}</replace>
<!-- Solve spamming of stations -->
<!-- Which Cue is this? - UniTrader -->
<add sel="//do_if[@value='$ShipyardCount lt $DesiredShipyards']" pos="before">
<do_if value="$ShipyardCount lt $DesiredShipyards">
	<set_value name="$DoubleCheckShipyardsCount" exact="0" />
	<find_station_by_true_owner name="$DoubleCheckPlannedShipyards" faction="$Faction" planneddefencestation="false" plannedshipyard="true" plannedwharf="false" plannedequipmentdock="false" plannedtradestation="false" space="player.galaxy" checkoperational="false" multiple="true"/>
	<do_if value="$DoubleCheckPlannedShipyards.count gt 0">
		<set_value name="$DoubleCheckShipyardsCount" exact="$DoubleCheckPlannedShipyards.count" />
	</do_if>
	<find_station_by_true_owner name="$DoubleCheckCompletedShipyards" faction="$Faction" space="player.galaxy" checkoperational="true" multiple="true">
		<match shipyard="true"/>
	</find_station_by_true_owner>
	<do_if value="$DoubleCheckPlannedShipyards and $DoubleCheckCompletedShipyards">
		<remove_from_list name="$DoubleCheckCompletedShipyards" list="$DoubleCheckPlannedShipyards" multiple="true"/>
	</do_if>
	<do_if value="$DoubleCheckCompletedShipyards.count gt 0">
		<set_value name="$DoubleCheckShipyardsCount" exact="$DoubleCheckCompletedShipyards.count" operation="add" />
	</do_if>
	<do_if value="$ShipyardCount lt $DoubleCheckShipyardsCount">
		<set_value name="$ShipyardCount" exact="$DoubleCheckShipyardsCount" />
	</do_if>
	<remove_value name="$DoubleCheckShipyardsCount" />
	<remove_value name="$DoubleCheckPlannedShipyards" />
	<remove_value name="$DoubleCheckCompletedShipyards" />
</do_if>
</add>
<!-- Which Cue is this? - UniTrader -->
